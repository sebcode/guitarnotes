{
  "extends": "rackt",
  "rules": {
    "valid-jsdoc": 2,
    "no-var": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-no-undef": 2,
    "react/jsx-wrap-multilines": 2,
    "react/jsx-uses-vars": 1,
    "comma-dangle": 0,
    "no-trailing-spaces": "error"
  },
  "plugins": [
    "react"
  ],
  "env": {
    "jquery": true
  },
  "globals": {
    "_": true,
    "__": true,
    "React": true
  }
}
